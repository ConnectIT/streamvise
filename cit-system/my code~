	char cmd[512];
	char tmpcmd[512];
        size_t len = 0;
	char *line = NULL;

	FILE *file;
	if (file = fopen("/tmp/readfromhere","r") )// Open the file
	{
	        while ((getline(&line, &len, file)) != -1) 
     		{
			sprintf(tmpcmd, "%s", &line[0]);
			printf("%s", tmpcmd);
			strcat(cmd,tmpcmd);
		}
		fclose(file);
	}
        y_cit_system_toastDone_send (cmd);
